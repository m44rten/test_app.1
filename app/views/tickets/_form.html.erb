<%= simple_form_for(@ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ticket_title %>
    <%= f.input :stamp %>
    <%= f.input :email_customer %>
    <%= f.input :tijd_gemeld %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
